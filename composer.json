{
  "name": "jacopo/laravel-authentication-acl",
  "description": "Laravel authentication and ACL package with a cool admin panel",
  "keywords": [
    "acl",
    "authentication",
    "signup",
    "admin panel",
    "profile",
    "laravel"
  ],
  "repositories": [
    {
      "type":"git",
      "url": "https://github.com/lidichamp/sentry.git"
    }
  ],
  "authors": [
    {
      "name": "jacopo",
      "email": "jacopo@jacopobeschi.com",
      "homepage": "http://www.jacopobeschi.com"
    },
    {
      "name": "Mustafa Segun",
      "email": "segsalerty@gmail.com"
    }
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/framework": "5.7.*",
    "laravelcollective/html" : "5.7.*",
    "intervention/image": "^2.4@dev",
    "gregwar/captcha": "dev-master",
    "jacopo/authentication-sentry": "dev-pronov",
    "nesbot/carbon": "^1.33"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "0.9.*",
    "fzaninotto/faker": "~1.4",
    "sebastian/exporter": "^3.1"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "LaravelAcl\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/unit"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled"
    ],
    "post-create-project-cmd": [
      "php -r \"copy('.env.example', '.env');\"",
      "php artisan key:generate"
    ]
  },
  "minimum-stability": "dev",
  "config": {
    "preferred-install": "dist"
  }
}
